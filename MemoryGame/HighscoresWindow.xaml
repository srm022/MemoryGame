<Window x:Class="MemoryGame.HighscoresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemoryGame"
        mc:Ignorable="d"
        Title="Highscores" Height="257.5" Width="183.75">
    <Grid>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="172" Margin="11,12,0,0" VerticalAlignment="Top" Width="154">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="closeButton" Content="Close" HorizontalAlignment="Left" Margin="51,194,0,0" VerticalAlignment="Top" Width="75" Click="closeButton_Click"/>

    </Grid>
</Window>
